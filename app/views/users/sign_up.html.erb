<h1>User Sign up </h1>

<%= form_for(User.new, url: '/sign_up', method: 'post') do |f| %>

    <div>
      <%= f.label :email%>
      <%= f.text_field :email %>
    </div>
    <div> 
      <%= f.label :password , "password"%>
      <%= f.password_field :password %>     
    </div>
    <div>
      <%= f.label :password_confirm, "password_confirm"%> 
      <%= f.password_field :password_confirm%>
    </div>

    <div>
      <%= f.submit "註冊"%>
    </div>
    
    

<%end%>

